<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="360"
                android:centerColor="#FCD6A2"
                android:endColor="#BA2F9C"
                android:startColor="#F99545"
                android:type="sweep" />
        </shape>
    </item>
    <item android:top="5dp" android:bottom="5dp" android:right="5dp" android:left="5dp">
        <shape android:shape="oval">
            <size android:width="90dp"
                android:height="90dp"/>
            <solid android:color="#fff"/>
        </shape>
    </item>
</layer-list>